{
  "name": "aquariumspecs",
  "version": "0.0.1",
  "description": "Website for aquariumspecs.info",
  "main": "server.js",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "pretty-quick": "pretty-quick",
    "test": "yarn eslint && yarn build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pierceray/aquariumspecs.git"
  },
  "author": "Ray Pierce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pierceray/aquariumspecs/issues"
  },
  "homepage": "https://github.com/pierceray/aquariumspecs#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "next": "^8.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run pretty-quick && npm run eslint"
    }
  }
}
