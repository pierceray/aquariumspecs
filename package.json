{
  "name": "aquariumspecs",
  "version": "0.0.1",
  "description": "Website for aquariumspecs.info",
  "main": "server.js",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "pretty-quick": "pretty-quick",
    "test": "npm run eslint && npm run build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pierceray/aquariumspecs.git"
  },
  "author": "Ray Pierce",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pierceray/aquariumspecs/issues"
  },
  "homepage": "https://github.com/pierceray/aquariumspecs#readme",
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "prettier": "1.15.1",
    "pretty-quick": "^1.8.0"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.0",
    "next": "^7.0.2",
    "node-sass": "^4.9.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run pretty-quick && npm run eslint"
    }
  }
